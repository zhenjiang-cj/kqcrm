<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC
       "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
       "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">
<struts-config>

	<form-beans>
       <form-bean name="approvalFlowForm"  type="com.nl.integratedManage.actionForm.ApprovalFlowForm"></form-bean>
    </form-beans>
            
	<action-mappings>
	
		<action path="/approvalFlowAction" type="com.nl.integratedManage.action.ApprovalFlowAction" parameter="method"
		 name="approvalFlowForm" scope="request" validate="false">						
			<forward name="doApply" path="/coremain/integratedManage/doApply.jsp" />
			<forward name="doingApply" path="/coremain/integratedManage/doingApply.jsp" />
			<forward name="noApply" path="/coremain/integratedManage/noApply.jsp" />
			<forward name="queryApplyInfo" path="/coremain/integratedManage/queryApplyInfo.jsp" />
			<forward name="queryMyDeal" path="/coremain/integratedManage/queryMyDeal.jsp" />
			<forward name="queryMyRead" path="/coremain/integratedManage/queryMyRead.jsp" />
			<forward name="queryMyTrace" path="/coremain/integratedManage/queryMyTrace.jsp" />			
			<forward name="dealOrder" path="/coremain/integratedManage/dealOrder.jsp" />
			<forward name="queryExpireCert" path="/coremain/integratedManage/queryExpireCert.jsp" />
			<forward name="queryCertInfo" path="/coremain/integratedManage/queryCertInfo.jsp" />
			<forward name="toUpdateCert" path="/coremain/integratedManage/toUpdateCert.jsp" />
			
		</action>
		
		<action path="/frontManageAction" type="com.nl.integratedManage.action.FrontManageAction" parameter="method"
		 name="approvalFlowForm" scope="request" validate="false">						
			<forward name="queryFrontCertInfo" path="/coremain/integratedManage/queryFrontCertInfo.jsp" />
			
		</action>
						
	</action-mappings>
</struts-config>
