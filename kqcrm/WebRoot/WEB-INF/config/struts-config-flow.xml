<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC
       "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
       "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">
<struts-config>
	<global-forwards>
		
	</global-forwards>

	<form-beans>
       <form-bean name="flowForm"  type="com.nl.sysflow.actionForm.FlowForm"/>
    </form-beans>
	          
	<action-mappings>
	
		<action path="/flowAction" type="com.nl.sysflow.action.FlowAction" parameter="method"
		 name="flowForm" scope="request" validate="false">
		    <forward name="tosubmmit" path="/coremain/flow/flow_submit.jsp"/>
		    <forward name="toMyWaitDeal" path="/coremain/flow/flow_waitdeal_task_list.jsp"/>
		    <forward name="toFlowInfoDetail" path="/coremain/flow/flow_task_info_view.jsp"/>
		    <forward name="toDealWait" path="/coremain/flow/flow_task_deal_main.jsp"/>
		    <forward name="toMyWaitRead" path="/coremain/flow/flow_waitread_task_list.jsp"/>
		    <forward name="toFlowTrack" path="/coremain/flow/flow_task_tracklist.jsp"/>
		    <forward name="toFlowReport" path="/coremain/flow/flow_report.jsp"/>
		    
			<forward name="success" path="/coremain/flow/success.jsp"/> 
		</action>
		
	</action-mappings>

	<message-resources parameter="struts-config_res.properties" />
</struts-config>
