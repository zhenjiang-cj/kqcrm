<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC
       "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
       "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">
<struts-config>

	<form-beans>
       <form-bean name="documentManageForm"  type="com.nl.documentManage.actionForm.DocumentManageForm"></form-bean>
    </form-beans>
    
        
	<action-mappings>
	
		<action path="/documentManage" type="com.nl.documentManage.action.DocumentManageAction" parameter="method"
		 name="documentManageForm" scope="request" validate="false">			
			<forward name="query" path="/coremain/documentManage/docContent.jsp" />
			<forward name="queryList" path="/coremain/documentManage/queryDoc.jsp" />
			<forward name="detail" path="/coremain/documentManage/docDetail.jsp" />
			<forward name="upload" path="/coremain/documentManage/uploadDoc.jsp" />
			<forward name="success" path="/coremain/documentManage/success.jsp" />
		</action>
						
	</action-mappings>
</struts-config>
