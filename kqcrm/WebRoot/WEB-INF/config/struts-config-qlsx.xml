<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC
       "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
       "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">
<struts-config>
	<global-forwards>
		
	</global-forwards>

	<form-beans>
       <form-bean name="qlInfoForm"  type="com.nl.ql.actionForm.QlForm"/>
    </form-beans>
	          
	<action-mappings>
	
		<action path="/qlsx" type="com.nl.ql.action.QlManageAction" parameter="method"
		 name="qlInfoForm" scope="request" validate="false">
		    
			<forward name="successList" path="/coremain/qlsx/qlInfoList.jsp"/>
			<forward name="successDetail" path="/coremain/qlsx/qlInfoDetail.jsp"/>
			<forward name="successVisit" path="/coremain/qlsx/foot.jsp"/>
		</action>
		
		<action path="/qlsxBack" type="com.nl.ql.action.QlBackManageAction" parameter="method"
		 name="qlInfoForm" scope="request" validate="false">
		    
			<forward name="successAdd" path="/coremain/qlsx/qlInfoAddBack.jsp"/>
			<forward name="successList" path="/coremain/qlsx/qlInfoListBack.jsp"/>
			<forward name="successModify" path="/coremain/qlsx/qlInfoModifyBack.jsp"/>
		</action>
		
	</action-mappings>

	<message-resources parameter="struts-config_res.properties" />
</struts-config>
